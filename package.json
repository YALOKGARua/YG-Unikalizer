{
  "name": "yg-unikalizer",
  "version": "4.0.0",
  "private": true,
  "main": "electron/main.js",
  "author": "YALOKGARIA <yalokgar@gmail.com>",
  "description": "YG Unikalizer â€“ unique and anonymize photos with smart drifts and rich fake EXIF/IPTC.",
  "license": "MIT",
  "repository": {
    "type": "git",
    "url": "git+https://github.com/YALOKGARua/YG-Unikalizer.git"
  },
  "homepage": "https://github.com/YALOKGARua/YG-Unikalizer#readme",
  "bugs": {
    "url": "https://github.com/YALOKGARua/YG-Unikalizer/issues"
  },
  "keywords": [
    "photo",
    "anonymize",
    "exif",
    "iptc",
    "metadata",
    "electron",
    "react",
    "windows"
  ],
  "scripts": {
    "wasm:build": "asc wasm/assembly/index.ts -b public/wasm/pu.wasm -t public/wasm/pu.wat --optimize --noAssert",
    "wasm:watch": "npm run wasm:build -- --watch",
    "rust:build": "cd rust-native && npm install && npm run build",
    "rust:build:debug": "cd rust-native && npm run build:debug",
    "rust:install": "cd rust-native && npm install",
    "dev": "npm run electron:build && npm run native:build && concurrently -k \"vite\" \"wait-on http://localhost:5173 && cross-env VITE_DEV_SERVER_URL=http://localhost:5173 electron .\"",
    "build": "vite build",
    "start": "electron .",
    "chat:server": "node server/chat-server.js",
    "chat:build": "esbuild server/chat-server.ts --platform=node --format=cjs --target=node18 --bundle --outfile=server/chat-server.js",
    "mobile:server": "node server/mobile-sync-server.js",
    "mobile:build": "esbuild server/mobile-sync-server.ts --platform=node --format=cjs --target=node18 --bundle --outfile=server/mobile-sync-server.js --external:sharp --external:express --external:socket.io --external:multer --external:uuid",
    "mobile:check": "curl -s http://localhost:3030/api/session/create || echo 'Mobile server not running'",
    "game:server": "node server/game-server.js",
    "game:build": "esbuild server/game-server.ts --platform=node --format=cjs --target=node18 --bundle --outfile=server/game-server.js --external:ws",
    "dist": "npm run build && npm run mobile:build && npm run native:build && npm run rust:build && electron-builder",
    "publish": "npm run build && npm run mobile:build && npm run native:build && npm run rust:build && electron-builder --publish always",
    "release:patch": "npm version patch -m \"release: %s\" && git push",
    "release:minor": "npm version minor -m \"release: %s\" && git push",
    "release:major": "npm version major -m \"release: %s\" && git push",
    "postinstall": "electron-builder install-app-deps && node scripts/build-native.js && node scripts/build-electron.js && npm run mobile:build",
    "native:build": "node scripts/build-native.js",
    "electron:build": "node scripts/build-electron.js",
    "native:prebuild": "cd native && prebuildify --napi --strip",
    "assets:icons": "node scripts/make-icons.js",
    "open:sendto": "powershell -NoProfile -Command \"Start-Process explorer.exe ($env:APPDATA+'\\\\Microsoft\\\\Windows\\\\SendTo')\""
  },
  "dependencies": {
    "@dnd-kit/core": "^6.3.1",
    "@dnd-kit/sortable": "^10.0.0",
    "@emnapi/core": "^1.5.0",
    "@emnapi/runtime": "^1.5.0",
    "@formkit/auto-animate": "0.9.0",
    "@headlessui/react": "2.2.8",
    "@iconify/react": "^6.0.2",
    "@radix-ui/react-icons": "^1.3.2",
    "@react-spring/web": "^10.0.3",
    "@tanstack/react-query": "^5.90.2",
    "@tanstack/react-virtual": "^3.13.12",
    "@tippyjs/react": "4.2.6",
    "@types/react-syntax-highlighter": "^15.5.13",
    "canvas-confetti": "^1.9.3",
    "clsx": "2.1.1",
    "cmdk": "^1.1.1",
    "dotenv": "^17.2.2",
    "electron-store": "^10.1.0",
    "electron-updater": "^6.6.2",
    "exifr": "^7.1.3",
    "express": "^4.21.2",
    "framer-motion": "^12.23.19",
    "i18next": "^25.5.2",
    "lottie-react": "^2.4.1",
    "lucide-react": "0.544.0",
    "mermaid": "^11.12.0",
    "multer": "^1.4.5-lts.1",
    "node-addon-api": "^8.5.0",
    "node-gyp-build": "^4.8.4",
    "prismjs": "^1.30.0",
    "qrcode": "^1.5.4",
    "react": "^19.1.1",
    "react-confetti": "^6.4.0",
    "react-countup": "^6.5.3",
    "react-dnd": "^16.0.1",
    "react-dnd-html5-backend": "^16.0.1",
    "react-dom": "^19.1.1",
    "react-dropzone": "^14.3.8",
    "react-hook-form": "^7.63.0",
    "react-hot-toast": "^2.6.0",
    "react-i18next": "^15.7.3",
    "react-icons": "^5.5.0",
    "react-intersection-observer": "^9.16.0",
    "react-is": "^19.1.1",
    "react-loading": "^2.0.3",
    "react-markdown": "^10.1.0",
    "react-parallax-tilt": "^1.7.309",
    "react-particles": "^2.12.2",
    "react-ripples": "^2.2.1",
    "react-router-dom": "^7.9.1",
    "react-syntax-highlighter": "^15.6.6",
    "react-use": "^17.6.0",
    "react-window": "^2.1.1",
    "recharts": "^3.2.1",
    "rehype-highlight": "^7.0.2",
    "remark-gfm": "^4.0.1",
    "sharp": "^0.34.4",
    "socket.io": "^4.8.1",
    "socket.io-client": "^4.8.1",
    "sonner": "2.0.7",
    "swiper": "^12.0.2",
    "tailwind-merge": "3.3.1",
    "tailwindcss": "^3.4.17",
    "tippy.js": "6.3.7",
    "uuid": "^11.0.5",
    "ws": "^8.18.3",
    "zustand": "^5.0.8"
  },
  "devDependencies": {
    "@tailwindcss/container-queries": "^0.1.1",
    "@tailwindcss/forms": "^0.5.10",
    "@tailwindcss/typography": "^0.5.18",
    "@types/express": "^5.0.3",
    "@types/multer": "^1.4.12",
    "@types/node": "^24.5.2",
    "@types/qrcode": "^1.5.5",
    "@types/react": "^19.1.13",
    "@types/react-dom": "^19.1.9",
    "@types/uuid": "^10.0.0",
    "@vitejs/plugin-react": "^5.0.3",
    "asc": "npm:assemblyscript@^0.28.8",
    "assemblyscript": "^0.28.8",
    "autoprefixer": "^10.4.21",
    "concurrently": "^9.2.1",
    "cross-env": "^10.0.0",
    "electron": "^38.1.2",
    "electron-builder": "^26.0.12",
    "esbuild": "^0.25.10",
    "node-gyp": "^11.4.2",
    "png-to-ico": "^3.0.1",
    "postcss": "^8.5.6",
    "prebuildify": "^6.0.1",
    "react-responsive": "^10.0.1",
    "tailwind-scrollbar": "^3.1.0",
    "tailwindcss-animate": "^1.0.7",
    "typescript": "^5.9.2",
    "vite": "^7.1.7",
    "wait-on": "^9.0.1"
  },
  "overrides": {
    "wrappy": "1.0.2",
    "once": {
      "wrappy": "1.0.2"
    },
    "inflight": {
      "wrappy": "1.0.2"
    }
  },
  "build": {
    "appId": "com.yalokgaria.yg-unikalizer",
    "productName": "YG Unikalizer",
    "artifactName": "YG-Unikalizer-${version}-${os}-${arch}.${ext}",
    "copyright": "Copyright (C) 2025 YALOKGAR",
    "files": [
      "dist/**",
      "electron/**",
      "server/**",
      "public/mobile/**",
      "node_modules/**",
      "native/prebuilds/**",
      "native/build/Release/*.node",
      "rust-native/index.node",
      "rust-native/*.node",
      "rust-native/package.json",
      "CHANGELOG.md",
      "README.md",
      "package.json",
      "public/wasm/pu.wasm",
      "!**/.vscode/**",
      "!**/.github/**",
      "!**/test/**",
      "!rust-native/target/**",
      "!rust-native/node_modules/**"
    ],
    "compression": "maximum",
    "asar": true,
    "asarUnpack": [
      "node_modules/sharp/**/*",
      "native/build/Release/*.node",
      "native/prebuilds/**",
      "rust-native/*.node",
      "server/**/*"
    ],
    "directories": {
      "buildResources": "build"
    },
    "win": {
      "executableName": "YG-Unikalizer",
      "icon": "build/icon.ico",
      "requestedExecutionLevel": "requireAdministrator",
      "target": [
        {
          "target": "nsis",
          "arch": [
            "x64"
          ]
        }
      ]
    },
    "nsis": {
      "oneClick": true,
      "perMachine": true,
      "artifactName": "YG-Unikalizer-Setup-${version}.exe",
      "include": "build/installer.nsh"
    }
  }
}
